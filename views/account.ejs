<%- include('partials/header'); -%>

<div class="account-page-outer">
    <div class="account-page-inner">

        <div class="home-link">
            <p> <a href="/feed">Home </a> >><span><%=user.name%></span> </p>
        </div>

        <div class="user-profile">
            <div class="user-name-img">
                <img src="/images/mongalmoy_img.jpg" alt="_image">
                <p>&emsp;<strong><%=user.name%></strong></p>
                <!-- <div class="user-img">
                    <img src="/images/mongalmoy_img.jpg" alt="_image">
                </div>
                <div class="user-name">
                    Mongalmoy Karmakar
                </div> -->
            </div>

            <hr class="main-hr">

            <div class="user-profile-details">
                <p> <span>Email:&emsp;&emsp;<strong><%=user.username%></span> </strong></p>
                <hr class="p-br">

                <p> <span>Country:&emsp;&emsp;<strong>India</span> </strong></p>
                <hr class="p-br">

                <p> <span>State:&emsp;&emsp;<strong>West Bengal</span> </strong></p>
                <hr class="p-br">

                <p> <span>City:&emsp;&emsp;<strong>Kolkata</span> </strong></p>
                <hr class="p-br">

                <p> <span>Current Position:&emsp;&emsp;<strong><%=user.currorg%></span> </strong></p>
                <hr class="p-br">

                <p> <span>Year:&emsp;&emsp;<strong><%=user.year%></span> </strong></p>
                <hr class="p-br">

                <p> <span>Batch:&emsp;&emsp;<strong><%=user.department%></span></strong> </p>
                <hr class="p-br">
            </div>
        </div>

    </div>
</div>

<%- include('partials/footer'); -%>